{
  "name": "shocker-server",
  "version": "1.0.0",
  "description": "Node.js server for shock endpoint API",
  "main": "server.js",
  "author": "ShockerServer",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "generate-cert": "node generate-cert.js",
    "generate-keys": "node generate-api-keys.js",
    "setup": "npm run generate-cert && npm run generate-keys && npm start",
    "letsencrypt": "node server.js",
    "production": "NODE_ENV=production node server.js",
    "production-root": "sudo NODE_ENV=production node server.js"
  },
  "keywords": ["shocker", "api", "nodejs", "express"],
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "https": "^1.0.0",
    "fs": "^0.0.1-security",
    "acme-client": "^4.2.0",
    "dotenv": "^16.3.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
